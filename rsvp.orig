<script>
$(() => {
  const render = () => {
    const attending = $('input[name=attend][value=yes]:checked').length > 0;
    const renderGuest = (index) => {
      const hasName = !!$(`input[name=name${index}]`).val().trim();
      const $email = $(`input[name=email${index}]`);
      const $radio = $(`input[name=entree${index}]`);
      const needEntree = attending && hasName;
      $email.attr('required', hasName);
      $radio.attr('required', needEntree);
      $radio.closest('.entree').css('visibility', needEntree ? '' : 'hidden');
    };
    renderGuest(1);
    renderGuest(2);
  };
  $('input').on('change keydown keyup', render);
  render();
});
</script></head><body><div class="wf-section"><div class="container840 headerimg w-container"><img src="https://uploads-ssl.webflow.com/607f7748fc8bd376e3ed6ac3/6086497f7ad6171e071f562d_Header.png" loading="lazy" sizes="(max-width: 991px) 100vw, 900px" srcset="https://uploads-ssl.webflow.com/607f7748fc8bd376e3ed6ac3/6086497f7ad6171e071f562d_Header-p-1080.png 1080w, https://uploads-ssl.webflow.com/607f7748fc8bd376e3ed6ac3/6086497f7ad6171e071f562d_Header-p-1600.png 1600w, https://uploads-ssl.webflow.com/607f7748fc8bd376e3ed6ac3/6086497f7ad6171e071f562d_Header.png 1800w" alt="" class="img-dsk"/><img src="https://uploads-ssl.webflow.com/607f7748fc8bd376e3ed6ac3/608b749d7d89dfb530dc3451_HeaderB2.png" loading="lazy" sizes="(max-width: 767px) 100vw, (max-width: 991px) 91vw, 100vw" srcset="https://uploads-ssl.webflow.com/607f7748fc8bd376e3ed6ac3/608b749d7d89dfb530dc3451_HeaderB2-p-800.png 800w, https://uploads-ssl.webflow.com/607f7748fc8bd376e3ed6ac3/608b749d7d89dfb530dc3451_HeaderB2-p-1080.png 1080w, https://uploads-ssl.webflow.com/607f7748fc8bd376e3ed6ac3/608b749d7d89dfb530dc3451_HeaderB2.png 1588w" alt="" class="img-tab"/><img src="https://uploads-ssl.webflow.com/607f7748fc8bd376e3ed6ac3/608b93f4c88c574db2f109fe_HeaderB3.png" loading="lazy" sizes="(max-width: 479px) 100vw, (max-width: 767px) 94vw, 100vw" srcset="https://uploads-ssl.webflow.com/607f7748fc8bd376e3ed6ac3/608b93f4c88c574db2f109fe_HeaderB3-p-800.png 800w, https://uploads-ssl.webflow.com/607f7748fc8bd376e3ed6ac3/608b93f4c88c574db2f109fe_HeaderB3-p-1080.png 1080w, https://uploads-ssl.webflow.com/607f7748fc8bd376e3ed6ac3/608b93f4c88c574db2f109fe_HeaderB3.png 1320w" alt="" class="img-mbl-land"/><img src="https://uploads-ssl.webflow.com/607f7748fc8bd376e3ed6ac3/609ca93786c0819a6bf9947c_HeaderB4.png" loading="lazy" sizes="(max-width: 479px) 90vw, 100vw" srcset="https://uploads-ssl.webflow.com/607f7748fc8bd376e3ed6ac3/609ca93786c0819a6bf9947c_HeaderB4-p-500.png 500w, https://uploads-ssl.webflow.com/607f7748fc8bd376e